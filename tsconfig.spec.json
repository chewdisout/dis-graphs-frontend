{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine"
    ],
    "baseUrl": "src",
    "paths": {
      "@core/*": ["app/core/*"],
      "@services/*": ["app/services/*"],
      "@features/*": ["app/features/*"],
      "@models/*": ["app/models/*"]
    }
  },
  "include": [
    "src/**/*.ts"
  ]
}
